{
  "version": 2,
  "name": "phonenumbers",
  "builds": [{ "src": "*.py", "use": "@now/python", "config": { "maxLambdaSize": "15mb" }}],
  "routes": [
    { "src": "/api/possible/((%2B)?[0-9]+)/([xA-Z]{2})", "dest":"/possible.py?N=$1&C=$3"},
    { "src": "/api/valid/([+]?[0-9]+)/([A-Z]{2}|None)", "dest":"/valid.py?N=$1&C=$2"},
    { "src": "/api/format/([+]?[0-9]+)/(None|[A-Z]{2})/(INT|NAT|E164)", "dest":"/format.py?N=$1&C=$2&T=$3"}
  ]
}
